{
  // -------------------- CONFIGURAÇÕES COMUNS --------------------
  "HttpPort": 80,
  "HttpServerEnabled": true,
  "OrthancExplorerEnabled": true,
  "RemoteAccessAllowed": true,
  "AuthenticationEnabled": true,
  "StoreDicom": true,
  "DefaultEncoding": "Utf8",
  "StorageDirectory": "/root/orthanc-storage",
  "TemporaryDirectory": "/root/orthanc-cache",
  "MallocArenaMax": 5,
  "SaveJobs": false,
  "Plugins": [
    "/usr/local/share/orthanc/plugins/"
  ],
  "PythonScript": "/root/conf/script.py",
  "MySQL": {
    "EnableSsl": false,
    "EnableIndex": true,
    "EnableStorage": false,
    "Host": "balanced-reader-db",
    "Port": 3306,
    "Database": "otc",
    "Username": "otc",
    "Password": "otc",
    "Lock": false,
    "MaximumConnectionRetries": 5,
    "ConnectionRetryInterval": 5,
    "IndexConnectionsCount": 2
  },
  // -------------------- CONFIGURAÇÕES COMUNS --------------------
  "RegisteredUsers": {
    "###ORTHANC_USER###": "###ORTHANC_USER_PASSWORD###"
  },
  "WebViewer": {
    "ShowInfoPopupAtStartup": "never",
    "DefaultLanguage": "pt",
    "StudyDownloadEnabled": true,
    "VideoDisplayEnabled": true,
    "AnnotationStorageEnabled": false,
    "KeyImageCaptureEnabled": false,
    "ShortTermCacheEnabled": true,
    "ShortTermCachePath": "WebViewerCache",
    "ShortTermCacheSize": 4000,
    "ShortTermCachePrefetchOnInstanceStored": false,
    "ShortTermCacheThreads": 32,
    "ShortTermCacheDebugLogsEnabled": false,
    "HighQualityImagePreloadingEnabled": true
  },
  "DicomServerEnabled": false,
  "HttpCompressionEnabled": true,
  "LimitFindResults": 25,
  "LimitFindInstances": 25,
  "HttpTimeout": 10,
  "KeepAlive": true,
  "TcpNoDelay": true,
  "HttpThreadsCount": 150,
  "QueryRetrieveSize": 10,
  "CaseSensitivePN": false
}